<table>
  <tr><th>Food</th><th>Measurement unit</th><th>Unit price</th><th>quantity</th><th>Action</th></tr>
  <% @my_foods.each do |food| %>
  <tr>
    <td><%= food.name%></td>
    <td><%= food.measurement_unit%></td>
    <td><%= food.price%></td>
    <td><%= food.quantity%></td>
    <td><%= button_to 'Delete', user_food_path(id: food), method: :delete%></td>
  </tr>
  <% end %>

<% @user_one.food.each do |food| %>
  <% food.recipe_foods.each do |fd|%>
  <%= fd.id%>

  <% end %>

<% end %>

</table>
    <%= button_to 'Add new food', new_user_food_path, method: :get%>
    <%= button_to 'Home', user_path(current_user), method: :get%>
